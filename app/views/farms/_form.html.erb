<%= form_for(@farm) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :name, "Farm Name" %>
  <%= f.text_field :name %>

  <%= f.fields_for :blocks do |builder| %>
    <%= render 'block_fields', f: builder %>
  <% end %>

  <%= f.submit "Save", class: "btn btn-large btn-primary" %>
<% end %>